extends ../_pug-includes/layout

block vars
    - var active = 'docs'  
    - var bodyAttrs = {'data-spy': "scroll", 'data-target': "#components-nav", 'data-offset':"80"}
    - var path = '../'    
    - var breadcrumbs = []
    - breadcrumbs.push({name: 'Home', link: '../index.html'})
    - breadcrumbs.push({name: 'Changelog'})
    - var heroHeading = 'Changelog'
    - var current = 'docs/changelog.html'

block content

    +breadcrumbs(breadcrumbs)
    +pageHeader(heroHeading) 

    section.mb-5 
        .row
            .col-xl-10
                .card
                    .card-header
                    .card-body 

